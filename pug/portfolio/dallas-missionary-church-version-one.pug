extends ../templates/template.pug
block title&description
  meta(name="description" content="Assure Studios portfolio piece one")
  title Assure Studios-Portfolio Piece Dallas Missionary Church version one
block links
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC' crossorigin='anonymous')
  link(rel='stylesheet', href='../css/main.css')

block bodyTag
  -var bodyTag = "portfolio-piece"
  body(id=`${bodyTag}`)
block header
  header#header.headerTop
    .logo
      a(href='../')
        img.logo-img(width="104px", height="104px" src='../../img/logo.png' alt="assure studios logo")
        span Assure Studios
    .mobile-menu-toggler
      svg(viewbox='0 0 100 80' width='40' height='40')
        rect(width='100' height='4')
        rect(y='30' width='100' height='4')
        rect(y='60' width='100' height='4')
    nav.nav-bar
      .mobile-close
        span x
      .nav-link.home-link
        a(href="../")  home
        hr
      .nav-link.about-link
        a(href="../about.html") about
        hr
      .nav-link
        a(href="../portfolio.html") portfolio
        hr
      .nav-link.headerTel
         tel(title="Give us a call") phone: 972-898-7128
         hr
      .nav-link.headerButton
        button.header-button(onClick="location.href='../contact.html'"  title="Free Consultation") Free Consultation

block content
    img(src="../img/Dallas-missionary-church-version-one.png")
    .project-section.about-project
      span.project-num 01
      .project-title About the Project
      .project-content
        h1 Dallas Missionary Church version one
        p  Dallas Missionary Church is a fictional church created by Assure Studios to create a mock-up of a modern church website I have three versions of this website this is the first one.
    .project-section.typography
      span.project-num 02
      .project-title typography
      .typo-container(style="font-family: 'Montserrat'; font-weight: 600;")
        .typo-box
          .typo-name Montserrat
          .typo-letters AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz
          .typo-numbers 1234567890
    .project-section.project-colors
      span.project-num 03
      .project-title Project Colors
      .project-color-container
        .project-color-box(style='background-color: #177fb4; color: #fff;') #177FB4
        .project-color-box(style='background-color: #0e0f12;') #0E0F12
        .project-color-box(style='background-color: #f6f6f6;') #F6F6F6
    .project-section.container-fluid
      .project-image.row
        .project-image-container.col-lg-12.col-xxl-8
          span.project-num 04
          .project-title-mockup Desktop Mock-up
          img.alignnone.size-full.wp-image-1887(src='../img/Dallas-missionary-church-version-one-desktop.png', alt='', width='1920', height='4820')
        .project-image-container.col-lg-12.col-xxl-4
          span.project-num 05
          .project-title-mockup Mobile Mock-up
          img(src='../img/Dallas-missionary-church-version-one-mobile.png', alt='')
block footer
  footer.footer
    .container
      .row.justify-content-between
        link(href='//cdn-images.mailchimp.com/embedcode/slim-10_7_dtp.css' rel='stylesheet' type='text/css')
        style(type='text/css').
        #mc_embed_signup
          form#mc-embedded-subscribe-form.validate(action='https://assurestudios.us9.list-manage.com/subscribe/post?u=8350d94f5f6838b1419783e02&id=b398063b79' method='post' name='mc-embedded-subscribe-form' target='_blank' novalidate='')
            #mc_embed_signup_scroll
              label(for='mce-EMAIL') Subscribe To Our Newsletter
              p Lets talk about web design, development, digital marketing and search engine optimization.
              .dflex
                input#mce-EMAIL.email(type='email' value='' name='EMAIL' placeholder='email address' required='')
                input#mc-embedded-subscribe.button.assure-button(type='submit' value='Subscribe' name='subscribe')

              // real people should not fill this in and expect good things - do not remove this or risk form bot signups
              div(style='position: absolute; left: -5000px;' aria-hidden='true')
                input(type='text' name='b_8350d94f5f6838b1419783e02_b398063b79' tabindex='-1' value='')

              .optionalParent
                .clear.foot
                  p.brandingLogo
                    a(href='http://eepurl.com/hPWYVr' title='Mailchimp - email marketing made easy and fun')
                      img(src='https://eep.io/mc-cdn-images/template_images/branding_logo_text_dark_dtp.svg')
      hr
      .social
        a.social(href="https://www.facebook.com/assurestudios") Facebook
        a.social(href="https://twitter.com/assurestudios") Twitter
        a.social(href="https://www.instagram.com/assure_studios/") Instagram
      .copyright
        a(href="../privacy-policy.html") Privacy Policy
        a(href="../terms-and-conditions.html") Terms and Conditions
        p Â© copyright 2022 by Assure Studios


  block scripts
    link(rel='stylesheet', href='https://use.typekit.net/mtg4ahu.css')
    link(href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet")
    script(src="../js/main.js")
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js' integrity='sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM' crossorigin='anonymous')
    script(src="https://unpkg.com/aos@2.3.1/dist/aos.js")
    script AOS.init();
